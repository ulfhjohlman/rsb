target 'rsb' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for rsb
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'GoogleSignIn'
  
  target 'rsbTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'rsbUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
